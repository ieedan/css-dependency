{
	"name": "css-dependency",
	"packageManager": "pnpm@9.15.4",
	"version": "0.0.1",
	"description": "A css parser who's only job is to get the imports.",
	"type": "module",
	"scripts": {
		"build": "tsup",
		"lint": "biome lint --write",
		"format": "biome format --write",
		"check": "biome check",
		"test": "vitest",
		"changeset": "changeset",
		"ci:release": "unbuild && changeset publish"
	},
	"keywords": [
		"css"
	],
	"files": [
		"dist/**/*",
		"src/**/*",
		"package.json",
		"pnpm-lock.json",
		"README.md"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.1",
		"@changesets/cli": "^2.27.8",
		"tsup": "^8.3.6",
		"vitest": "^2.1.1"
	},
	"dependencies": {
		"ansi-regex": "^6.1.0"
	}
}
